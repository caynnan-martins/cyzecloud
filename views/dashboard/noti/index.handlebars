<link rel="stylesheet" href="css/dashboard.css">
{{>_sidebars}}

<section class="bg-white dark:bg-[#141e27]">
    <div class="p-4 sm:ml-64">
      <div class="p-4 mt-14 flex flex-col gap-5">
        
        {{#each Noti}}
            {{#unless this.lidas}}
                   <div class="flex items-start gap-2.5">
   <img class="w-8 h-8 rounded-full" src="assets/logo.png" alt="Jese image">
   <div class="flex flex-col gap-1 w-full max-w-[320px]">
      <div class="flex items-center space-x-2 rtl:space-x-reverse">
         <span class="text-sm font-semibold text-gray-900 dark:text-white">Cyze Cloud</span>
         <span class="bg-blue-100 text-blue-800 text-xs font-medium me-2 px-2.5 py-0.5 rounded dark:bg-blue-900 dark:text-blue-300">Oficial</span>
         <span class="text-sm font-normal text-gray-500 dark:text-gray-400">{{this.horario}}</span>
      </div>
      <div class="flex flex-col leading-1.5 p-4 border-gray-200 bg-gray-100 rounded-e-xl rounded-es-xl dark:bg-gray-700">
         <p class="text-sm font-normal text-gray-900 dark:text-white">{{this.content}}</p>
      </div>
      <span class="text-sm font-normal text-gray-500 dark:text-gray-400">Sistema - Automatico</span>
   </div>
</div>
            {{/unless}}
        {{/each}}

      </div>
    </div>
</section>